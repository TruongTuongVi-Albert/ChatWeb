{
    "builds": [
      {
        "src": "angular.json", 
        "use": "@vercel/static-build",
        "config": { "distDir": "dist/webchat-cloud" }
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
  